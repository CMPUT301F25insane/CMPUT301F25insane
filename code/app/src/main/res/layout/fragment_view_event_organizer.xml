<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frameLayout2"
        android:layout_width="367dp"
        android:layout_height="509dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/org_view_profile_image" >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="334dp"
            android:layout_height="124dp"
            android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.484"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/event_name_for_org_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/cambria"
                android:textSize="25dp"
                android:text="Event Name"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/orgEventViewEventDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="Date and Time"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/event_name_for_org_view" />

            <TextView
                android:id="@+id/registration_deadline_text_org_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="13dp"
                android:text="Registration Deadline"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/orgEventViewEventDate" />

            <TextView
                android:id="@+id/host_name_org_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="12dp"
                android:text="host Name"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/host_profile_image_org_view"
                app:layout_constraintVertical_bias="0.311" />

            <ImageView
                android:id="@+id/host_profile_image_org_view"
                android:layout_width="55dp"
                android:layout_height="60dp"
                android:layout_marginEnd="16dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:srcCompat="@tools:sample/avatars" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/event_location_org_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="16dp"
            android:text="Location"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout" />

        <TextView
            android:id="@+id/event_description_org_view"
            android:layout_width="274dp"
            android:layout_height="68dp"
            android:layout_marginTop="12dp"
            android:text="Description"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.494"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/event_location_org_view" />

        <Button
            android:id="@+id/qr_button_org_view"
            android:layout_width="111dp"
            android:layout_height="55dp"
            android:layout_marginEnd="36dp"
            android:text="View QR Code"
            app:layout_constraintBottom_toBottomOf="@+id/attendees_org_view"
            app:layout_constraintEnd_toStartOf="@+id/attendees_org_view"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/event_description_org_view"
            app:layout_constraintVertical_bias="1.0" />

        <Button
            android:id="@+id/attendees_org_view"
            android:layout_width="121dp"
            android:layout_height="52dp"
            android:layout_marginEnd="28dp"
            android:text="View Attendees"
            app:layout_constraintEnd_toStartOf="@+id/photos_org_view"
            app:layout_constraintTop_toBottomOf="@+id/event_description_org_view" />

        <Button
            android:id="@+id/photos_org_view"
            android:layout_width="76dp"
            android:layout_height="45dp"
            android:text="View Photos"
            app:layout_constraintBottom_toBottomOf="@+id/attendees_org_view"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/event_description_org_view"
            app:layout_constraintVertical_bias="1.0" />

        <Button
            android:id="@+id/event_edit_button_ord_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Edit"
            tools:layout_editor_absoluteX="263dp"
            tools:layout_editor_absoluteY="445dp" />

        <Button
            android:id="@+id/delete_button_org_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Delete"
            tools:layout_editor_absoluteX="11dp"
            tools:layout_editor_absoluteY="454dp" />

        <com.google.android.gms.maps.MapView
            android:id="@+id/mapView"
            android:layout_width="348dp"
            android:layout_height="103dp"
            tools:layout_editor_absoluteX="11dp"
            tools:layout_editor_absoluteY="331dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/header_for_view_event_organizer"
        android:layout_width="130dp"
        android:layout_height="39dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/cambria_italic"
        android:text="Camaraderie"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/name_of_organizer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="[Name]"
        android:fontFamily="@font/cambria"
        app:layout_constraintEnd_toStartOf="@+id/org_view_profile_image"
        app:layout_constraintHorizontal_bias="0.887"
        app:layout_constraintStart_toEndOf="@+id/header_for_view_event_organizer"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/org_view_profile_image"
        android:layout_width="58dp"
        android:layout_height="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />
    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="411dp"
        android:layout_height="120dp"
        android:background="@color/black"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="610dp">

        <ImageButton
            android:id="@+id/hostEvent"
            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/black"
            android:src="@drawable/hostevent" />

        <ImageButton
            android:id="@+id/dashboardButton"
            android:layout_width="143dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/black"
            android:scaleX="1.3"
            android:scaleY="1.3"
            android:src="@drawable/dashboard_image" />

        <ImageButton
            android:id="@+id/myEvents"
            android:layout_width="136dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/black"
            android:src="@drawable/seeevents" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>