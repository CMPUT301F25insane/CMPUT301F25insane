<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient_resource"
    >

    <TextView
        android:id="@+id/Camaraderie_header_for_create_events"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/cambria_italic"
        android:text="Camaraderie"
        android:textColor="@color/white"
        android:textSize="23dp"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.352" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="381dp"
        android:layout_height="789dp"
        android:background="@drawable/gradient_for_org_view"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.782">

        <EditText
            android:id="@+id/input_field_for_create_event_location"
            android:layout_width="129dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="32dp"
            android:ems="25"
            android:fontFamily="@font/cambria_italic"
            android:hint="[Location]"
            android:inputType="text"
            android:textColor="@color/white"
            android:textColorHint="#9C9C9C"
            app:layout_constraintBottom_toTopOf="@+id/input_field_for_create_event_description"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.111"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/input_field_for_create_event_num_of_attendees"
            android:layout_width="178dp"
            android:layout_height="48dp"
            android:layout_marginBottom="28dp"
            android:ems="25"
            android:fontFamily="@font/cambria_italic"
            android:hint="[Number of Attendees]"
            android:inputType="text"
            android:textColor="@color/white"
            android:textColorHint="#9C9C9C"
            app:layout_constraintBottom_toTopOf="@+id/button_for_going_back"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.046"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/input_field_for_create_event_waitlist_limit"
            android:layout_width="175dp"
            android:layout_height="46dp"
            android:layout_marginBottom="28dp"
            android:ems="25"
            android:fontFamily="@font/cambria_italic"
            android:hint="[Optional Waitlist Limit]"
            android:inputType="text"
            android:textColor="@color/white"
            android:textColorHint="#9C9C9C"
            android:textSize="17dp"
            app:layout_constraintBottom_toTopOf="@+id/button_for_confirm"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.529"
            app:layout_constraintStart_toEndOf="@+id/input_field_for_create_event_num_of_attendees" />

        <EditText
            android:id="@+id/input_field_for_create_event_description"
            android:layout_width="303dp"
            android:layout_height="144dp"
            android:layout_marginBottom="52dp"
            android:ems="25"
            android:fontFamily="@font/cambria_italic"
            android:hint="[Description]"
            android:inputType="text"
            android:textColor="@color/white"
            android:textColorHint="#9C9C9C"
            app:layout_constraintBottom_toTopOf="@+id/button_for_add_picture"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.358"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/input_field_for_create_event_name"
            android:layout_width="129dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="44dp"
            android:ems="25"
            android:fontFamily="@font/cambria_italic"
            android:hint="[Event Name]"
            android:inputType="text"
            android:textColor="@color/white"
            android:textColorHint="#9C9C9C"
            app:layout_constraintBottom_toTopOf="@+id/input_field_for_create_event_registration_deadline"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.082"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <EditText
            android:id="@+id/input_field_for_create_event_time"
            android:layout_width="121dp"
            android:layout_height="40dp"
            android:layout_marginTop="144dp"
            android:ems="25"
            android:fontFamily="@font/cambria_italic"
            android:hint="[Time]"
            android:textColor="@color/white"
            android:textColorHint="#9C9C9C"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.54"
            app:layout_constraintStart_toEndOf="@+id/input_field_for_create_event_date"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/input_field_for_create_event_registration_deadline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp"
            android:clickable="true"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:fontFamily="@font/cambria_italic"
            android:hint="Registration Deadline"
            android:inputType="none"
            android:textColor="@color/white"
            android:textColorHint="#9C9C9C"
            android:textSize="17dp"
            app:layout_constraintBottom_toTopOf="@+id/input_field_for_create_event_date"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.124"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/input_field_for_create_event_date"
            android:layout_width="173dp"
            android:layout_height="33dp"
            android:layout_marginStart="28dp"
            android:layout_marginBottom="24dp"
            android:clickable="true"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:fontFamily="@font/cambria_italic"
            android:hint="Date"
            android:inputType="none"
            android:textColor="@color/white"
            android:textColorHint="#9C9C9C"
            android:textSize="17dp"
            app:layout_constraintBottom_toTopOf="@+id/input_field_for_create_event_location"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/button_for_add_picture"
            android:layout_width="146dp"
            android:layout_height="53dp"
            android:layout_marginBottom="56dp"
            android:backgroundTint="#8FE5E4FF"
            android:fontFamily="@font/cambria"
            android:text="Add Picture"
            android:textColor="@color/black"
            android:textSize="17dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/input_field_for_create_event_num_of_attendees"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:strokeColor="@color/black"
            app:strokeWidth="3.5dp" />

        <Button
            android:id="@+id/button_for_going_back"
            android:layout_width="146dp"
            android:layout_height="53dp"
            android:layout_marginBottom="24dp"
            android:backgroundTint="#8FE5E4FF"
            android:fontFamily="@font/cambria"
            android:text="Back"
            android:textColor="@color/black"
            android:textSize="17dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.118"
            app:layout_constraintStart_toStartOf="parent"
            app:strokeColor="@color/black"
            app:strokeWidth="3.5dp" />

        <Button
            android:id="@+id/button_for_confirm"
            android:layout_width="146dp"
            android:layout_height="53dp"
            android:layout_marginBottom="24dp"
            android:backgroundTint="#B06CF9CC"
            android:fontFamily="@font/cambria"
            android:text="Confirm"
            android:textColor="@color/black"
            android:textSize="17dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.464"
            app:layout_constraintStart_toEndOf="@+id/button_for_going_back"
            app:strokeColor="@color/black"
            app:strokeWidth="3.5dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>