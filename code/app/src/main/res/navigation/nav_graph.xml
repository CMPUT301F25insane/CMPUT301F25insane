<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/fragment_loading_screen">

    <fragment
        android:id="@+id/fragment_loading_screen"
        android:name="com.example.camaraderie.utilStuff.LoadingScreenFragment"
        android:label="Loading screen Fragment"
        tools:layout="@layout/fragment_loading_screen">

        <action
            android:id="@+id/action_fragment_loading_screen_to_fragment_main"
            app:destination="@id/fragment_main"/>
        <action
            android:id="@+id/action_fragment_loading_screen_to_admin_main_screen"
            app:destination="@id/admin_main_screen" />
        <action
            android:id="@+id/action_fragment_loading_screen_to_fragment_pending_events"
            app:destination="@id/fragment_pending_events" />

    </fragment>

    <fragment
        android:name="com.example.camaraderie.event_screen.organizer_view.OrganizerNotificationFragment"
        android:id="@+id/organizerNotificationFragment"
        android:label="organizer notification screen"
        tools:layout="@layout/fragment_organizer_notification_dialog_test">

        <action
            android:id="@+id/action_organizerNotificationFragment_to__fragment_organizer_view_event"
            app:destination="@id/_fragment_organizer_view_event" />
    </fragment>

    <fragment
        android:name="com.example.camaraderie.my_events.ViewMyCreatedEventsFragment"
        android:id="@+id/fragment_my_created_events"
        tools:layout="@layout/fragment_my_created_events">

        <action
            android:id="@+id/action_fragment_my_created_events_to_fragment_view_my_events"
            app:destination="@id/fragment_view_my_events" />
    </fragment>

    <fragment
        android:id="@+id/fragment_guidelines"
        android:label="guidleins FRagment"
        android:name="com.example.camaraderie.updateUserStuff.GuidelinesFragment"
        tools:layout="@layout/fragment_guidelines">

        <action
            android:id="@+id/action_fragment_guidelines_to_update_user"
            app:destination="@id/update_user" />
    </fragment>

    <fragment
        android:id="@+id/fragment_pending_events"
        android:name="com.example.camaraderie.accepted_screen.UserAcceptedToEventFragment"
        android:label="Pending events fragment"
        tools:layout="@layout/fragment_pending_events">

        <action
            android:id="@+id/action_fragment_pending_events_to_fragment_main"
            app:destination="@id/fragment_main"/>

    </fragment>

    <fragment
        android:id="@+id/fragment_view_my_events"
        android:name="com.example.camaraderie.my_events.ViewMyEventsFragment"
        android:label="View My Events"
        tools:layout="@layout/fragment_view_my_events">


        <action
            android:id="@+id/action_fragment_view_my_events_to_fragment_main"
            app:destination="@id/fragment_main" />
        <action
            android:id="@+id/action_fragment_view_my_events_to_fragment_create_event_testing"
            app:destination="@id/fragment_create_event_testing" />
        <action
            android:id="@+id/action_fragment_view_my_events_to__fragment_organizer_view_event"
            app:destination="@id/_fragment_organizer_view_event" />
        <action
            android:id="@+id/action_fragment_view_my_events_to_fragment_my_created_events"
            app:destination="@id/fragment_my_created_events" />
        <action
            android:id="@+id/action_fragment_view_my_events_to_update_user"
            app:destination="@id/update_user" />
    </fragment>

    <fragment
        android:id="@+id/_fragment_organizer_view_event"
        android:name="com.example.camaraderie.event_screen.organizer_view.OrganizerViewEventFragment"
        android:label="Organizer event view"
        tools:layout="@layout/fragment_view_event_organizer">


        <action
            android:id="@+id/action__fragment_organizer_view_event_to_fragment_main"
            app:destination="@id/fragment_main" />
        <action
            android:id="@+id/action__fragment_organizer_view_event_to_fragment_create_event_testing"
            app:destination="@id/fragment_create_event_testing" />
        <action
            android:id="@+id/action__fragment_organizer_view_event_to_fragment_view_my_events"
            app:destination="@id/fragment_view_my_events" />
        <action
            android:id="@+id/action__fragment_organizer_view_event_to_map"
            app:destination="@id/map" />

        <argument
            android:name="eventDocRefPath"
            app:argType="string" />

        <action
            android:id="@+id/action__fragment_organizer_view_event_to_fragment_organizer_view_photos"
            app:destination="@id/fragment_organizer_view_photos" />
        <action
            android:id="@+id/action__fragment_organizer_view_event_to_fragment_list_testing_interface2"
            app:destination="@id/fragment_list_testing_interface" />
        <action
            android:id="@+id/action__fragment_organizer_view_event_to_update_user"
            app:destination="@id/update_user" />
        <action
            android:id="@+id/action__fragment_organizer_view_event_to_organizerNotificationFragment"
            app:destination="@id/organizerNotificationFragment" />

    </fragment>

    <fragment
        android:id="@+id/fragment_organizer_view_photos"
        android:name="com.example.camaraderie.image_stuff.OrganizerViewPhotosFragment"
        android:label="Organizer View Photos"
        tools:layout="@layout/fragment_organizer_view_photos">

    </fragment>

    <fragment
        android:id="@+id/fragment_main"
        android:name="com.example.camaraderie.dashboard.MainFragment"
        android:label="Main Fragment"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_fragment_main_to_fragment_create_event_testing"
            app:destination="@id/fragment_create_event_testing" />
        <action
            android:id="@+id/action_fragment_main_to_fragment_view_event_user"
            app:destination="@id/fragment_view_event_user" />
        <action
            android:id="@+id/action_fragment_main_to_fragment_view_my_events"
            app:destination="@id/fragment_view_my_events" />
        <action
            android:id="@+id/action_fragment_main_to_update_user"
            app:destination="@id/update_user" />
        <action
            android:id="@+id/action_fragment_main_to__fragment_organizer_view_event"
            app:destination="@id/_fragment_organizer_view_event" />
    </fragment>

    <fragment
        android:id="@+id/fragment_create_event_testing"
        android:name="com.example.camaraderie.event_screen.organizer_view.CreateEventFragment"
        android:label="Create Event Fragment"
        tools:layout="@layout/fragment_create_event">
        <action
            android:id="@+id/action_fragment_create_event_testing_to_fragment_main"
            app:destination="@id/fragment_main" />
        <action
            android:id="@+id/action_fragment_create_event_testing_to__fragment_organizer_view_event"
            app:destination="@id/_fragment_organizer_view_event" />
    </fragment>

    <fragment
        android:id="@+id/fragment_view_event_user"
        android:name="com.example.camaraderie.event_screen.user_view.UserViewEventFragment"
        android:label="View Event User Fragment"
        tools:layout="@layout/fragment_view_event_user">

        <argument
            android:name="eventDocPath"
            app:argType="string" />
        <argument
            android:name="userDocPath"
            app:argType="string" />
        <action
            android:id="@+id/action_fragment_view_event_user_to_fragment_main"
            app:destination="@id/fragment_main" />
        <action
            android:id="@+id/action_fragment_view_event_user_to_fragment_create_event_testing"
            app:destination="@id/fragment_create_event_testing" />
        <action
            android:id="@+id/action__fragment_view_event_user_to_fragment_view_my_events"
            app:destination="@id/fragment_view_my_events" />
        <action
            android:id="@+id/action_fragment_view_event_user_to_fragment_view_waitlist"
            app:destination="@id/fragment_view_waitlist_or_selected" />
        <action
            android:id="@+id/action_fragment_view_event_user_to_fragment_list_testing_interface"
            app:destination="@id/fragment_list_testing_interface" />
        <action
            android:id="@+id/action_fragment_view_event_user_to_update_user"
            app:destination="@id/update_user" />

        <action android:id="@+id/action__fragment_user_view_event_to_fragment_user_view_photos"
            app:destination="@id/fragment_user_view_photos"/>

    </fragment>

    <fragment
        android:id="@+id/fragment_user_view_photos"
        android:name="com.example.camaraderie.image_stuff.UserViewPhotosFragment"
        android:label="User View Photos"
        tools:layout="@layout/fragment_user_view_photos"/>

    <fragment
        android:id="@+id/fragment_view_waitlist_or_selected"
        android:name="com.example.camaraderie.event_screen.user_lists.waitlist_or_selected.ViewWaitlistOrSelectedFragment"
        android:label="View Waitlist Fragment"
        tools:layout="@layout/fragment_view_waitlist_or_selected">

        <action
            android:id="@+id/action_fragment_view_waitlist_or_selected_to_fragment_list_testing_interface"
            app:destination="@id/fragment_list_testing_interface" />
        <action
            android:id="@+id/action_fragment_view_waitlist_or_selected_to_fragment_view_event_user"
            app:destination="@id/fragment_view_event_user" />
    </fragment>


    <fragment
        android:name="com.example.camaraderie.event_screen.user_lists.ListTestingInterfaceFragment"
        android:id="@+id/fragment_list_testing_interface"
        android:label="testing organizers view of lists"
        tools:layout="@layout/fragment_view_lists_testing_interface">

        <action
            android:id="@+id/action_fragment_list_testing_interface_to__fragment_organizer_view_event"
            app:destination="@id/_fragment_organizer_view_event" />
        <action
            android:id="@+id/action_fragment_list_testing_interface_to_fragment_view_waitlist"
            app:destination="@id/fragment_view_waitlist_or_selected" />
        <action
            android:id="@+id/action_fragment_list_testing_interface_to_view_accepted_or_cancelled"
            app:destination="@id/view_accepted_or_cancelled" />
    </fragment>

    <fragment
        android:name="com.example.camaraderie.event_screen.user_lists.accepted_or_cancelled_list.ViewAcceptedOrCancelledFragment"
        android:id="@+id/view_accepted_or_cancelled"
        android:label="View accepted or cancelled fragment"
        tools:layout="@layout/fragment_view_accepted_or_cancelled">


        <action
            android:id="@+id/action_view_accepted_or_cancelled_to_fragment_list_testing_interface"
            app:destination="@id/fragment_list_testing_interface" />
    </fragment>



    <fragment
        android:id="@+id/admin_main_screen"
        android:name="com.example.camaraderie.admin_screen.AdminDashboardFragment"
        android:label="Admin Main Screen"
        tools:layout="@layout/fragment_admin_dashboard">

        <action
            android:id="@+id/action_admin_main_screen_to_update_user"
            app:destination="@id/update_user" />
        <action
            android:id="@+id/action_admin_main_screen_to_admin_user_data_screen_view"
            app:destination="@id/admin_user_data_screen_view" />
        <action
            android:id="@+id/action_admin_main_screen_to_admin_event_data_screen_view"
            app:destination="@id/admin_event_data_screen_view" />
        <action
            android:id="@+id/action_admin_main_screen_to_admin_event_images_screen_view"
            app:destination="@id/admin_event_images_screen_view" />
    </fragment>

    <fragment
        android:id="@+id/admin_user_data_screen_view"
        android:name="com.example.camaraderie.admin_screen.AdminUsersViewFragment"
        android:label="Admin User Screen"
        tools:layout="@layout/fragment_admin_users_view">

        <action
            android:id="@+id/action_admin_user_data_screen_view_to_admin_main_screen"
            app:destination="@id/admin_main_screen" />
        <action
            android:id="@+id/action_admin_user_data_screen_view_to_admin_user_profile"
            app:destination="@id/admin_user_profile" />
    </fragment>

    <fragment
        android:id="@+id/admin_event_data_screen_view"
        android:name="com.example.camaraderie.admin_screen.AdminEventsViewFragment"
        android:label="Admin Events Screen"
        tools:layout="@layout/fragment_admin_events_view">

        <action
            android:id="@+id/action_admin_event_data_screen_view_to_admin_main_screen"
            app:destination="@id/admin_main_screen" />
    </fragment>

    <fragment
        android:id="@+id/admin_event_images_screen_view"
        android:name="com.example.camaraderie.admin_screen.AdminImagesViewFragment"
        android:label="Admin Images Screen"
        tools:layout="@layout/fragment_admin_images_view">

        <action
            android:id="@+id/action_admin_event_images_screen_view_to_admin_main_screen"
            app:destination="@id/admin_main_screen" />
    </fragment>

    <fragment
        android:id="@+id/admin_user_profile"
        android:name="com.example.camaraderie.admin_screen.AdminUserProfileFragment"
        android:label="User Profile Admin"
        tools:layout="@layout/fragment_admin_user_profile">
        <action
            android:id="@+id/action_admin_user_profile_to_admin_user_data_screen_view"
            app:destination="@id/admin_user_data_screen_view" />
    </fragment>


    <fragment
        android:id="@+id/update_user"
        android:name="com.example.camaraderie.updateUserStuff.UpdateUserFragment"
        android:label="Update Screen"
        tools:layout="@layout/fragment_update_user">

        <action
            android:id="@+id/action_update_user_to_admin_main_screen"
            app:destination="@id/admin_main_screen" />
        <action
            android:id="@+id/action_update_user_to_fragment_main"
            app:destination="@id/fragment_main" />
        <action
            android:id="@+id/action_update_user_to_fragment_guidelines"
            app:destination="@id/fragment_guidelines" />
    </fragment>

    <fragment
        android:id="@+id/map"
        android:name="com.example.camaraderie.event_screen.LocationMapFragment"
        android:label="map"
        tools:layout="@layout/fragment_location_map">
        <action
            android:id="@+id/action_map_to__fragment_organizer_view_event"
            app:destination="@id/_fragment_organizer_view_event" />
    </fragment>



</navigation>
